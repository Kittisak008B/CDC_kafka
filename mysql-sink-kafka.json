      "errors.log.include.messages": true,
      "connection.attempts": "6",
      "connection.backoff.ms": "1000",
      "connection.url": "jdbc:mysql://34.41.134.91:3306/database_demo02?nullCatalogMeansCurrent=true&autoReconnect=true&useSSL=false",
